<form method="post" action="<%=action%>">
	<input type="hidden" name="id" value="<%=restaurant.id%>">
	<input type="text" name="name" placeholder="Restaurant Name" value="<%=restaurant.name%>">
	<input type="text" name="longitude" placeholder="Longitude" value="<%=restaurant.longitude%>">
	<input type="text" name="latitude" placeholder="Latitude" value="<%=restaurant.latitude%>">
	<button type="submit" class="btn btn-default">Submit</button>
</form>